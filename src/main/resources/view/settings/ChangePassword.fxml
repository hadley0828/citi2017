<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.settingController.ChangePasswordController">
   <children>
      <Label layoutX="200.0" layoutY="110.0" text="原密码" />
      <Label layoutX="200.0" layoutY="231.0" text="现密码" />
      <PasswordField fx:id="rawPassword" layoutX="368.0" layoutY="105.0" />
      <PasswordField fx:id="newPassword" layoutX="368.0" layoutY="226.0" />
      <Button fx:id="saveButton" layoutX="200.0" layoutY="391.0" mnemonicParsing="false" onAction="#saveClicked" text="修改" />
      <Button fx:id="cancelButton" layoutX="446.0" layoutY="391.0" mnemonicParsing="false" onAction="#cancelClicked" text="取消" />
      <Label layoutX="206.0" layoutY="308.0" text="确认密码" />
      <PasswordField fx:id="confirmPassword" layoutX="368.0" layoutY="303.0" />
   </children>
</AnchorPane>
